RUN: ! test -f %t.nvptx
RUN: ! test -s %t.stdout
RUN: echo 'error opening ./GLOBAL_FUNCTION_DECL_f.o' > %t.stderr.golden
RUN: cmp %t.stderr.golden %t.stderr

